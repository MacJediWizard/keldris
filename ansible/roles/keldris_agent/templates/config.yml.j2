# Keldris Agent Configuration
# Managed by Ansible - do not edit manually
---
server_url: {{ keldris_server_url | to_json }}
api_key: {{ keldris_api_key | to_json }}
{% if keldris_hostname is defined and keldris_hostname %}
hostname: {{ keldris_hostname | to_json }}
{% endif %}
auto_check_update: {{ keldris_auto_check_update | bool | lower }}
{% if keldris_http_proxy is defined or keldris_https_proxy is defined or keldris_socks5_proxy is defined or keldris_no_proxy is defined %}
proxy:
{% if keldris_http_proxy is defined and keldris_http_proxy %}
  http_proxy: {{ keldris_http_proxy | to_json }}
{% endif %}
{% if keldris_https_proxy is defined and keldris_https_proxy %}
  https_proxy: {{ keldris_https_proxy | to_json }}
{% endif %}
{% if keldris_no_proxy is defined and keldris_no_proxy %}
  no_proxy: {{ keldris_no_proxy | to_json }}
{% endif %}
{% if keldris_socks5_proxy is defined and keldris_socks5_proxy %}
  socks5_proxy: {{ keldris_socks5_proxy | to_json }}
{% endif %}
{% endif %}
